{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Frontend Health",
      "type": "stat",
      "gridPos": { "h": 3, "w": 8, "x": 0, "y": 0 },
      "targets": [{ "expr": "up{job=\"frontend\"}" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ]
        }
      }
    },
    {
      "title": ".NET Service Health",
      "type": "stat",
      "gridPos": { "h": 3, "w": 8, "x": 8, "y": 0 },
      "targets": [{ "expr": "up{job=\"dotnet\"}" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ]
        }
      }
    },
    {
      "title": "AI Service Health",
      "type": "stat",
      "gridPos": { "h": 3, "w": 8, "x": 16, "y": 0 },
      "targets": [{ "expr": "up{job=\"ai\"}" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ]
        }
      }
    },
    {
      "title": "Excel Imports (success/failure)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 3 },
      "targets": [
        {
          "expr": "rate(excel_import_total{status=\"success\"}[5m])",
          "legendFormat": "Success"
        },
        {
          "expr": "rate(excel_import_total{status=\"failure\"}[5m])",
          "legendFormat": "Failure"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "Excel Import Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 3 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, rate(excel_import_duration_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(excel_import_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(excel_import_duration_seconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "PDF Report Generation",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 11 },
      "targets": [
        {
          "expr": "sum(rate(pdf_report_total[5m])) by (report_type, status)",
          "legendFormat": "{{ report_type }} ({{ status }})"
        }
      ]
    },
    {
      "title": "XML Generation",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 11 },
      "targets": [
        {
          "expr": "sum(rate(xml_generation_total[5m])) by (status)",
          "legendFormat": "XML {{ status }}"
        }
      ]
    },
    {
      "title": "AI Forecast Model Usage",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 19 },
      "targets": [
        {
          "expr": "ai_forecast_model_total",
          "legendFormat": "{{ model }}"
        }
      ]
    },
    {
      "title": "AI Request Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 19 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ai_request_duration_seconds_bucket[5m])) ",
          "legendFormat": "{{ endpoint }} p95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Anomalies Detected",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 19 },
      "targets": [
        {
          "expr": "sum(rate(ai_anomalies_detected_total[15m])) by (severity)",
          "legendFormat": "{{ severity }}"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["ecosfer", "services"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Ecosfer SKDM - Service Health",
  "uid": "ecosfer-service-health"
}
